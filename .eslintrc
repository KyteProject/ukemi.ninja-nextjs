{
	"extends": [
		"xo",
		"xo-nextjs",
		"plugin:import/errors",
		"plugin:react/recommended",
		"plugin:jsx-a11y/recommended",
		"prettier",
		"prettier/react"
	],
	"rules": {
		"react/prop-types": 0,
		"react/require-default-props": 0,
		"react/jsx-uses-vars": [
			2
		],
		"jsx-a11y/label-has-for": 0,
		"no-console": 0,
		"quotes": [
			"warn",
			"single"
		],
		"semi": [
			"warn",
			"always"
		]
	},
	"plugins": [
		"react",
		"import",
		"jsx-a11y"
	],
	"parser": "babel-eslint",
	"settings": {
		"react": {
			"createClass": "createReactClass",
			"pragma": "React",
			"version": "16.7.0",
			"flowVersion": "0.53",
		},
		"propWrapperFunctions": [
			"forbidExtraProps"
		]
	},
	"parserOptions": {
		"ecmaVersion": 8,
		"sourceType": "module",
		"ecmaFeatures": {
			"jsx": true,
			"classes": true,
			"impliedStrict": true
		}
	},
	"env": {
		"es6": true,
		"browser": true,
		"node": true,
		"commonjs": true
	}
}
